<!DOCTYPE html>
<html>
  <head>
    <%- include('./partials/head', {title: title}); %>
  </head>
  <body>
    <header>
      <%- include('./partials/header'); %>
    </header>
    <main>
      <div class='posts-container'>
      <% posts.forEach((post) => { %> 
        <div class='post-container'>
          <h2><%= post.title %></h2>
          <p><%= post.text %></p>
          <p><%= post.user.username %></p>
          <p><%= post.timestamp %></p>
        </div>
      <% }); %> 
      </div>
    </main>
  </body>
</html>
